  }
}

/* ==========================================================================
#Pagination
========================================================================== */

.pagination_bottom {
  width: 100%;
  padding: 0;
  margin: 0;

  span {
    display: inline-block;
    list-style:none;
    padding:0;
    margin:0;
    background-color: #fff;
    color: $brand-primary;
    font-weight: 400;
    border: 1px solid $gray-300;
    height:35px;
    width:35px;
    line-height: 35px;
    text-align: center;
    transition: all .3s ease-in-out;

    & + span {
      margin-left: .5rem;
    }

    &.next, &.prev {
      width:auto;
      padding:0 .5rem;
    }

    &:hover {
      background-color: $brand-secondary;
      border-color: $brand-secondary;
      color:#fff;

      > * {
        color:#fff;
        text-decoration: none;
      }
    }

    &.current {
      background-color: $brand-primary;
      border-color:$brand-primary;
      color: #fff;
    }
  }
}

