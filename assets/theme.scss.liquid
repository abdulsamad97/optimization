/* ==========================================================================
#Headroom
========================================================================== */ 

#site-header {
  background-color: #f4f1ed;
  position: fixed;
  z-index: 10;
  top:0;
  right: 0;
  left: 0;
  z-index:121;

  @include media-query($large-down) {
    border-bottom:3px solid #000;
    width:100vw;
  }

  &.headroom--not-top {
    will-change: transform;
    top:0;
    transform:translateY(-100%);
  }

  &.headroom {
    transition: all 200ms linear;
  }
  &.headroom--pinned {
    transform: translateY(0%);
    box-shadow:0 4px 4px 0px rgba(0, 0, 0, 0.3);
  }
  &.headroom--unpinned {
    //transform: translateY(-100%);
  }

  &.headroom--top {
    transform:none !important;
    box-shadow:0 4px 4px 0px rgba(0, 0, 0, 0.0);
  }
}

/* ==========================================================================
#Head Top Actions
========================================================================== */ 

.head-top-actions {
  padding:.3rem 0;

  .col > ul {
    display:flex;
    flex-direction:row;
    flex-wrap:wrap;
    justify-content: flex-end;
    list-style:none;
    padding:0;
    margin:0;
  }

  li {
    & + li {
      margin-left: 1rem;
    }
  }

  a {
    text-decoration: none;
    font-size:12px;
  }

  .header-currency-select {
    width:50px;

    @include media-query($medium-up) {
      margin-top: 3px !important;
    }

    .doubly-nice-select.right .list {
      margin-top: 6px !important;
    }
  }
}

/* ==========================================================================
#Head Assurance
========================================================================== */ 

.head-assurance {
  background-color: $brand-secondary;
  color:#fff;
  padding:.5rem 0;
  margin-bottom: 1rem;

  @include media-query($large-up) {
    padding:.7rem 0;
  }

  p {
    margin:0;
    text-transform: uppercase;
    font-weight:600;
    @extend .text-small;
    letter-spacing:2px;
  }
}


/* ==========================================================================
#Site Logo
========================================================================== */ 

.site-logo {
  margin-bottom: 0;

  @include media-query($small-down) {
    margin-left:-15px;
    margin-right:-15px;
  }

  a {
    display:block;
    vertical-align: middle;
  }

  svg {
    width:100%;
    height:100%;
    max-width:152px;
    fill:#000;
    vertical-align: inherit;
    transition:all .3s ease-in-out;

    @include media-query($large-up) {
      max-width:280px;
    }

  }

  .headroom--not-top & {
    svg {
      @include media-query($widescreen) {
        max-width:210px
      }
    }
  }

  img {
    @include media-query($large-up) {
      margin-top: -5px;
    }
  }
}

