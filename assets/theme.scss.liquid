    }

    &:last-of-type {
      border-bottom:1px solid $gray-300;
    }
  }

  select {
    display:inline-block;
    width:auto;
    border:0;
    background:none;
    text-transform:uppercase;
    @include px-to-rem(14);
    font-weight:500;
  }

  button {
    width:100%;
    border:none;
    text-transform:uppercase;
    background-color:#fff;
    cursor:pointer;

    &:focus {
      outline:0;
    }

    &.closeMobileEditProductMenu {
      font-family:$title-font;
      @include px-to-rem(25);
      color:#fff;
