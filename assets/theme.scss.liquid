/* ==========================================================================
#Rich Text Editor
========================================================================== */
.rte {
  @include px-to-rem(12);
  line-height: 24px;
  color: $body-font-color;
  font-weight: 500;

  &.size-guide-rte {
    p {
      @include media-query($large-up) {
        max-width: 60%;
        display: block;
        margin: 0 auto;
      }
    }
  }

  a {
    text-decoration: underline;
    color:$brand-primary;
  }

  img {
    height: auto;
    max-width:100%;
  }

  table {
    table-layout: fixed;
  }

  ul,
  ol {
    margin: 0 0 ($gutter / 2) $gutter;
  }

  // Match the styles from RTE nested lists
  ul {
    list-style: disc outside;

    ul {
      list-style: circle outside;

      ul {
        list-style: square outside;
      }
    }
  }

  p,a,li,span {
    @include px-to-rem(12);
    line-height: 24px;
    color: $body-font-color;
    font-weight: 500;
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
    margin-left: 0;
    list-style-position: inside;
  }
}

// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

.rte__map-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 100%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  margin-top: 40px;
  margin-bottom: 40px;

  table {
    width: 100%;
    display: block;

    tbody {
      width: 100%;
      display: block;
    }

    tr {
      display: flex;
      flex-direction: row;
      flex-wrap: wrap;
      justify-content: space-between;
      border-bottom: 1px solid $gray-300;

      &:last-of-type {
        border-bottom: 0px;
      }

      td {
        //flex: 1 1 auto;
        text-transform: uppercase;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        padding: 10px 0;
        @include px-to-rem(12);
        font-weight: 500;
        width: calc(100%/7);

        @include media-query($small-down) {
          @include px-to-rem(10);
        }

        &:first-of-type {
          @include px-to-rem(20);
          font-family: $title-font;

          @include media-query($small-down) {
            @include px-to-rem(16);
          }
        }
      }
    }
  }
}

/*============================================================================
Responsive tables, defined with .responsive-table on table element.
==============================================================================*/

.responsive-table {

  @include media-query($medium-up) {
    thead {
      th {
        padding: .5rem;
        border-bottom: 1px solid $gray-300;
      }
    }

    tbody {
      td {
        padding:.5rem;
      }
    }
  }

  tbody .responsive-table-row {
    background-color: $gray-100;

    &:nth-child(odd) {
      background-color:$gray-200;
    }
  }

  tfoot {
    td {
      font-weight:600;
      padding:.5rem 0;
    }
  }

  a {
    color:$brand-primary;
  }
}

@include media-query($medium-down) {
  .responsive-table {
    width: 100%;


    thead {
      display: none;
    }

    tr {
      display: block;
    } // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      padding: .5rem $gutter / 2;
      margin: 0;
    }

    td::before {
      display: block;
      content: attr(data-label);
      text-align: left;
      text-transform: uppercase;
      font-weight:600;
      font-size: 0.875rem;
    }
  } // Add a keyline between rows
}
