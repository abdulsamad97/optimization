========================================================================== */

.header-actions {
  display:flex;
  flex-direction:row;
  align-items: center;

  > [class^="header-"] {
    padding:.5rem;
    transition:all .3s ease-in-out;

    @include media-query(large-up) {
      padding:0 .38rem;
    }

    & + div {
      /*       margin-left: calc(1rem - .38rem); */
      margin-left: 0;
    }
