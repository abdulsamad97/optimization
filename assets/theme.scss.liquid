    padding:0;
    margin:0 -4px 0;
    max-width:304px;

    @include media-query($small-down) {
      @include px-to-rem(16);
    }

    li {
      display:flex;
      flex-direction:column;
      justify-content:center;
      align-items:center;
      margin:4px;
      border:1px solid $gray-500;
      color:$gray-500;
      min-width:30px;
      min-height:30px;
      font-weight:500;
      @include px-to-rem(12);
      cursor:not-allowed;
      transition:all .3s ease-in-out;
      padding-left:5px;
      padding-right:5px;
    }

    li[data-available="true"] {
      border:1px solid #000;
      color:#000;
      cursor:pointer;

      &:hover {
        background-color:#000;
        color:#fff;
      }
    }

    li.active {
      background-color:#000;
      color:#fff;
    }

    li.active[data-available="false"] {
      background-color:$gray-400;
      border-color:$gray-800;
    }
  }
}

/* ==========================================================================
#Product Preorder
========================================================================== */

.preorder-message {
  border:1px solid $gray-500;
  padding: .75rem 1.25rem;
  margin-top: 1rem;
  margin-bottom:1rem;
  color:#ff0000;
  min-height:53px;

  p {
    @include px-to-rem(10);
    margin-bottom:.5rem;
    text-transform:uppercase;
  }

  > p:last-of-type {
    margin-bottom:0;
  }

  &:empty {
    display:none;
  }
}

/* ==========================================================================
#Product Size Guide
========================================================================== */

body.template-product .product-detail .product-size-guide {
  display: block;
  text-align: center;
  text-transform: uppercase;    
  margin: 1rem 0 0;
  color: #000;
  font-size: 14px;
  font-size: 0.875rem;
  font-weight: normal;
  text-decoration: underline;
}

/* ==========================================================================
#Product Key Actions
========================================================================== */

body.template-product .product-key-actions, .product-quickview-frame .product-detail .product-key-actions {
  display:flex;
  flex-direction:row;
  flex-wrap:nowrap;
  margin-top:1rem;

  @include media-query($large-up) {
    margin-top:1.5rem;
  }

  #btn-basket {
    width:calc(100% - 61px) !important;
    margin-right:7px;

    @include media-query($large-up) {
      width:calc(100% - 64px) !important;
      margin-right:10px;
    }
  }

  .product-wishlist {
    width:54px;
    height:54px;
    border:2px solid $gray-700;
    border-radius:50%;
    overflow:hidden;

    div {
      height:52px !important;
      width:52px !important;
      padding:0px !important;
    }

    [role="button"] {
      display:flex !important;
      flex-direction:column;
      justify-content:center;
      align-items:center;
    }

    .fa {
      @include px-to-rem(22);
      margin-left:-3px;
    }
  }
}

/* ==========================================================================
#Product Button Basket
========================================================================== */

@keyframes rotating {
  from {
    -ms-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
  }

  to {
    -ms-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

#btn-basket {
  &.variant-not-available {
    background-color:$gray-400;
    border-color:$gray-800;
  }

  &.btn-adding {
    display:inline-flex;
    flex-direction:row;
    align-items:center;
    justify-content: center;
    background-color:#2b733c;
    border-color:#083a14;

    .icon {
      margin-left:1rem;
      animation: rotating 2s linear infinite;
    }
  }

  &.btn-added {
    display:inline-flex;
    flex-direction:row;
    align-items:center;
    justify-content: center;
    background-color: $successBtn;
    border-color:#0b541b;

    .icon {
      margin-left:1rem;
    }
  }
}

/* ==========================================================================
#Product Upsell
========================================================================== */

.product-upsell {
  margin-top:1rem;
  text-align:left;
  padding:.7rem 0rem;
  min-height:53px;
  display:flex;
  flex-direction:column;
  justify-content:center;

  .rte > * {
    @include px-to-rem(10);
    color:#000;
    line-height:1;

    &:last-of-type {
      margin:0;
    }
  }

  a {
    display:inline-block;
  }
}

.product-klarna-upsell {
  display:flex;
  text-align: left;
  margin-top:15px;
  width:100%;
}

/* ==========================================================================
#Product Additional Details
========================================================================== */

#product-additional-details {
  position:relative;
  margin-top:3rem;
  height:200px;
  overflow:hidden;

  @include media-query($large-up) {
    margin-top:1rem;
  }

  h2, h3, h4 {
    font-family:$title-font;
    text-transform:uppercase;
    @include px-to-rem(25);
  }

  .rte {
    p,li,a,span,td,strong {
      @include px-to-rem(12);
      font-weight:400;
    }

    strong {
      font-weight:500;
    }
  }

  &:before {
    position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    content:'';
    background: rgb(255,255,255);
    background: linear-gradient(0deg, rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
    z-index:1;
    transition:all .3s ease-in-out;
  }

  &.active:before {
    opacity:0;
    visibility:0;
    background-color:transparent;
    z-index:-1;
  }

}

/* ==========================================================================
#Product Recently Viewed
========================================================================== */

.product-recently-viewed {
  margin-top:2rem;

  h2:not(.product-title),h3:not(.product-title),h4:not(.product-title) {
    font-family:$title-font;
    @include px-to-rem(30);
    text-align:center;

    @include media-query($large-up) {
      text-align:left;
    }
  }

  .featured-products-slick {
    display:flex;
    flex-direction:row;
    align-items:center;
    margin-left:-2.5px;
    margin-right:-2.5px;

    &.slick-initialized {
      display:flex;
    }

    .product-listing {
      width:calc(25% - 5px);
      margin-left:2.5px;
      margin-right:2.5px;
    }

    .slick-dots {
      display:none !important;
    }
  }

  .product-listing:focus, .product-listing a:focus {
    outline:0;
  }
}

