/* ==========================================================================
#Site Navigation
========================================================================== */


#site-nav-desktop {
  border-top:1px solid $gray-100;
}

#site-nav-mobile {
  @include media-query($large-up) {
    position: fixed;
    top: 124px;
    left: 0;
    right: 0;
    margin: 0;
    background: #f4f1ed;
    width: 100%;
    max-width: none;
    z-index: 121;
  }
}

#site-navigation {
  width:100%;
  z-index:100;

  @include media-query($large-down) {
    position: fixed;
    //BG: Height of header on mobile
    top:57px;
    bottom:0;
    left:0;
    padding:0;
    background-color: #f4f1ed;
    transform:translateX(-100%);
    overflow-y:scroll;
    overflow-x:hidden;
    opacity:0;
    visibility:hidden;
    z-index:121;
    transition:all .4s ease-in-out;
  }

  @include media-query($large-up) {
    display:block !important;
    width:100%;
  }

  &.active {
    @include media-query($large-down) {
      transform:translateX(0);
      opacity:1;
      visibility:visible;
    }
  }


  //Generic styling
  ul {
    list-style:none;
    margin:0;
    padding:0;
  }

  a:not(.btn) {
    display:flex;
    flex-direction:row;
    align-items:center;
    text-decoration: none;

    @include media-query($large-up) {
      display:block;
    }

    &:hover {
      //text-decoration: underline;
      font-weight:500;
    }

    .plus-minus-icon {
      display:none;
    }
  }

  .has-child {

    .plus-minus-icon {
      @include media-query($large-down) {
        display:inline-block !important;
      }
    }

    .icon {
      @include media-query($large-up) {
        display:inline-block;
        width:12px;
        height:12px;
        display: none;
      }

      &.icon-chevron-right-light {
        display:inline-block;
        width:15px;
        height:15px;
        fill:$brand-primary;
        margin-left:auto;
        transition:all .3s ease-in-out;
        transform-origin: 7px 7px;

        @include media-query($large-up) {
          display:none;
        }
      }
    }
  }

  .single-menu {
    @include media-query($large-up) {
      position:relative;
    }

    .sub-menu {

      @include media-query($large-down) {
        li {
          border-top:1px solid #000;
          margin-left:15px;
          margin-right:15px;

          > a {
            padding:1.3rem 1rem;
            text-transform:uppercase;
          }
        }
      }

      @include media-query($large-up) {
        li a {
          @include px-to-rem(11);
          text-transform:uppercase;
        }
      }
    }
  }

  //Top Level
  > ul {
    display:flex;
    flex-direction:column;

    @include media-query($large-up) {
      flex-direction:row;
      justify-content:center;
    }

    > li {
      width:100%;

      @include media-query($large-up) {
        width:auto;
        padding-left:.5rem;
        padding-right:.5rem;

        &:hover > .sub-menu-container {
          transform: translateY(0);
          visibility: visible;
          opacity: 1;
          transition: all .3s ease-in-out;
        }
      }

      @include media-query($widescreen) {
        padding-left:1rem;
        padding-right:1rem;
      }

      &.active > a {
        @include media-query($large-up) {
          font-weight:500;
        }
      }

      > a {
        padding:.7rem 1rem;
        text-align:left;
        color:$body-font-color;
        transition:all .3s ease-in-out;
        text-transform: uppercase;

        @include media-query($large-down) {
          padding:1.3rem 1rem;
          @include px-to-rem(15);

          &:hover {
            text-decoration: none !important;
          }	
        }

        @include media-query($large-up) {
          text-align:center;
          padding:1.1rem .5rem;
          color:$body-font-color;
          @include px-to-rem(11);
          font-weight:400;
        }


        .icon {
          display:none;
        }
      }

      @include media-query($large-down) {
        &.active {

        }
      }
    }
  }

  //Sub Menu title
  .sub-menu-title {
    display: flex;
    flex-direction:row;
    align-items: center;
    padding:.7rem 25px;
    margin:0;
    font-family: $body-font;
    font-size:0.875rem;
    font-weight:600;
    color:$body-font-color;
    transition:all .3s ease-in-out;

    @include media-query($large-down) {
      border-top:1px solid $gray-600;
      font-weight:300;
      cursor: pointer;

      &:hover {

        a {
          text-decoration: none;
        }
      }
    }

    @include media-query($large-up) {
      display:block;
      padding:0;
      font-family: $title-font;
      @include px-to-rem(16);
      font-weight: 500;
      text-transform: uppercase;

      &:after {
        display:block;
        height:1px;
        width:100%;
        background-color: $body-font-color;
        margin:.5rem 0;
        content:'';
      }
    }

    &.active {

      svg.icon {
        transform:rotate(90deg);
      }
    }
  }

  //Sub Menu Row
  .sub-menu-row {
    display:flex;
    flex-direction:column;

    @include media-query($large-up) {
      flex-direction:row;
    }

    &.multiple {
      @include media-query($large-up) {
        display:inline-flex;
        flex-direction:column;
        width:49.5%;
      }
    }

    .sub-menu {
      width:100%;

      @include media-query($large-down) {
        > ul {

          li {
            border-top:1px solid #000;
            margin-left:0;
            margin-right:0;
          }

          a {
            @include px-to-rem(15);
            padding:1.3rem 1rem;
            text-transform:uppercase;
            transition:all .3s ease-in-out;

            &:hover {
              text-decoration: none;
            }
          }
        }

        &.active {

        }
      }

      @include media-query($large-up) {
        padding:0;

        li > a {
          @include px-to-rem(11);
          text-transform:uppercase;
        }
      }

      &.image {
        @include media-query($large-up) {
          display:flex;
          flex-direction:column;
          max-width:232px;

          img {
            transition:all .3s ease-in-out;
          }

          &:hover {
            img {
              filter: grayscale(100%);
            }
          }
        }

        .sub-menu-title {
          @include media-query($large-up) {
            margin-top:auto;
          }
        }
      }

      &.single {
        @include media-query($large-down) {
          > ul {
            display:block;

            > li > a {
              line-height:1rem;
              padding:.7rem 25px;
            }
          }
        }

        @include media-query($large-up) {
          padding-top:0;
          padding-bottom:0;
        }
      }

      .plus-minus-icon:before,.plus-minus-icon:after {
        background-color: #000;
      }
    }
  }

  .fixed-ratio-square {
    position: relative;
    display:block;
    width:100%;
    height:auto;
    overflow:hidden;
    text-decoration: none;
    padding-top:100% !important;

    > * {
      position: absolute;
      top:0;
      right:0;
      left:0;
      bottom:0;
      margin-right: auto !important;
      margin-left:auto !important;
      margin-top:auto !important;
      margin-bottom:auto !important;
      display:block;
      max-width:100%;
    }
  }

  //Sub Menu
  .sub-menu-container {
    display:none;
    width:100%;

    @include media-query($large-down) {
      overflow-x:hidden;
    }

    @include media-query($large-up) {
      display:block;
      position: absolute;
      left:10%;
      right:10%;
      max-width:80%;
      padding:2rem;
      background-color: rgba(#f4f1ed, .97);
      border-left:1px solid $gray-100;
      border-right:1px solid $gray-100;
      border-bottom:1px solid $gray-100;
      z-index:10;
      visibility: hidden;
      opacity: 0;
      transition: all .3s ease-in-out;
      min-width:170px;
    }

    a:not(.btn) {
      padding:.3rem 0;
      color:#000;

      @include media-query($large-up) {
        color:$body-font-color;
      }

      .icon {
        display:none;
      }
    }

    .go-to {
      padding:1.3rem 1rem !important;
      @include px-to-rem(15);
      text-transform:uppercase;
      transition:all .3s ease-in-out;

      @include media-query($large-up) {
        display:none;
      }

      > .icon {
        display:inline-block !important;
        margin-right:.5rem !important;
      }
    }

    .sub-menu {
      @include media-query($large-down) {
        background-color:#f4f1ed;
      }
    }
  }

  //Featured Menus
  .featured-menu-link {
    display:none;

    @include media-query($large-up) {
      display:block;
      position: relative;
      padding-right:1rem;
      width:100%;
    }

    .sub-menu-image {
      a {
        display:block;
        margin: 0;
        padding: 0;
      }

      img {
        transition: all .3s ease-in-out;
