    .icon-chevron-down {
      display:block;
      width:15px;
      height:15px;
      margin-left: .5rem;
      transition:all .3s ease-in-out;

      @include media-query($medium-up) {
        display:none;
      }
    }
  }

  .sidebar-content {
    display:none;

