    margin-top:3px;

  }

  &.glider {
    min-height:auto;

    .showcase-item {
      display:block;
    }
  }
}
.redirect-popup {
  position: fixed;
  top: 10%;
  left: 50%;
  transform: translateX(-50%);
  background: #fff;
  border: 1px solid $brand-secondary; 
  padding: 20px;
  visibility: hidden;
  opacity: 0;
  z-index:-1;
  transition:all .3s ease-in-out;
  width: calc(100% - 30px); 

  @include media-query($small-up) {
    width: auto;
    padding: 40px;
  }

  @include media-query($medium-up) {
    max-width: 480px;
    top: 50%;
    transform: translate(-50%, -50%);
  }

  &.active {
    visibility: visible;
    opacity:1;
    z-index:151;
  }

  .redirect-content {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;

    p {
      text-align: center;
    }

    .btn-redirect {
      margin-bottom: 1rem;
    }

    .redirect-close {
      text-transform: uppercase;
      //text-decoration: underline;
      cursor: pointer;
      font-size: 10px;
      text-align: center;

      &:hover {
        text-decoration: underline;
      }
    }
  }
}

.redirect-overlay {
  visibility: hidden;
  opacity: 0;
  position:fixed;
  top:0;
  right:0;
  bottom:0;
  left:0;
  background-color: rgba(#E4E4E4,.8);
  z-index:-1;
  transition:all .3s ease-in-out;

  &.active {
    visibility: visible;
    opacity:1;
    z-index:150;
  }
}

