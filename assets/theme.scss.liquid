      display:block;
      padding-top:1rem;
      padding-bottom:1rem;
      background-color: #fff;
      color:#000;
      border:1px solid #000;
      @include px-to-rem(22);
      text-transform:uppercase;
      text-decoration: none;

      &:hover {
        text-decoration: underline;
      }
    }
