      margin:0;

      &:focus {
        outline:0;
      }

      .fa {
        display:none;
      }

      &:after {
        display:inline-block;
        @include px-to-rem(14);
        text-transform:uppercase;
        color:#000;
      }

      &.unbookmarked:after {
        content:'Save for later';
      }

      &.bookmarked:after {
        content:'Saved';
      }

