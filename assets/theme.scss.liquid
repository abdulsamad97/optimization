      background-color: #fff;
      background-image: none !important;
    }

    @include media-query($medium-up) {
      position: relative;
      @include aspect-ratio(6,1);
      background-size: cover;
    }
  }

  h1 {
    text-transform: uppercase;
    @include media-query($medium-up) {
      position: absolute;
      bottom:0;
      left:0;
      right:0;
      margin-bottom:0;
      padding-top:.6rem;
      padding-bottom: .6rem;
      background-color: #fff;
    }
  }
