#shopify-section-mobile-collection-header {
  margin:0;
  position: relative;

  &:after {
    position: absolute;
    right:-15px;
    top:0;
    bottom:0;
    width:14%;
    background: rgb(255,255,255);
    background: linear-gradient(-90deg, #ffffff 20%, rgba(255,255,255,0) 100%);
    content:'';
    z-index:1;
  }

  @include media-query($large-up) {
    display:none;
    visibility: none;
    opacity:0;
  }
}

#mobile-collection-header {
  display:flex;
  flex-direction:row;
  margin:0px;
  min-height:100px;

  .showcase-item {
    display:none;
    margin:0 7.5px 0;
    //width:22.5%;
    padding:15px 0;

    @include media-query($small-down) {
      //width:20%;
      margin:0 7px 0;
    }

    &:first-of-type {
      margin-left:5px;
    }

    &:focus {
      outline:0;
      border:0;
    }

    a {
      text-decoration: none;

      &:focus {
        outline:0;
        border:0;
      }
    }
  }

  .showcase-item-image {
    display:block;
    width:59px;
    height:59px;
    overflow:hidden;
    border-radius:50%;
    padding:2px;
    border:2px solid #E3D5CC;
    margin:0 auto;

    img {
      width:auto;
      max-width:100%;
    }
