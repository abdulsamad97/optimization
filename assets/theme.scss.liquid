/* ==========================================================================
#Mobile Navigation Icon
========================================================================== */

.mobile-menu {
  position: relative;
  cursor: pointer;
  width:37px;
  height:37px;

  span {
    display:block;
  }

  &:hover {
    .burger {
      background-color: #000;

      &:before, &:after {
        background-color: #000;
      }
    }
  }

  &.active .burger {
    background-color: rgba(#000,0);
    z-index:101;

    &:before,&:after {
      background-color: #000;
    }

    &:before {
      top: -2px;
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);
    }

    &:after {
      bottom: 0;
      -webkit-transform: rotate(-45deg);
      transform: rotate(-45deg);
    }
  }

  .burger {
    position: absolute;
    background-color: #000;
    width: 20px;
    height: 1px;
    top: 50%;
    right: auto;
    left: auto;
    opacity: 1;
    -webkit-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
    //border-radius: 15px;
    height:0;

    &:before, &:after {
      position: absolute;
      display: block;
      background-color: #000;
      width: 20px;
      height: 2px;
      content: "";
      -webkit-transition: all .3s ease-in-out;
      transition: all .3s ease-in-out;
      //border-radius: 15px;
    }

    &:before {
      top: 4px;
      -webkit-transform: rotate(0);
      transform: rotate(0);
    }

    &:after {
      bottom: 4px;
      -webkit-transform: rotate(0);
      transform: rotate(0);
    }
  }
}


