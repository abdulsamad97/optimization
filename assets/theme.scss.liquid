/* ==========================================================================
#Basket Main
========================================================================== */
body.template-cart .title-bg {

  @include media-query($large-down) {
    display:flex;
    flex-direction:row;
    align-items:center;
    background-color:$gray-300;
    padding:15px;

    .continue-shopping {
      margin-left:auto;
      @include px-to-rem(9);
      text-transform:uppercase;
      color:#000;
    }
  }

  @include media-query($large-up) {
    .continue-shopping {
      display:none;
    }
  }
}

.bag-title {
  text-transform:uppercase;
  @include px-to-rem(30);
  margin:0;

  @include media-query($large-up) {
    @include px-to-rem(25);
    border-bottom:1px solid #000;
    padding-bottom:1.5rem;
    text-indent:1rem;
  }
}

/* ==========================================================================
#Basket Item
========================================================================== */

.basket-item {
  position:relative;
  display:flex;
  flex-direction:row;
  padding:1.5rem 0;

  &:after {
    /*position:absolute;
    bottom:0;
    left:20px;
    right:20px;
    display:block;
    height:1px;
    width:calc(100% - 40px);
    background-color:$gray-300;
    content:'';*/
  }

  &:last-of-type:after {
    //display:none;
  }

  &:first-child {
    //border-top:1px solid $gray-800;
  }

  @include media-query($medium-up) {
    flex-direction:row;
    padding:2rem 0;
  }

  .basket-item-image {
    width:25%;

    @include media-query($medium-up) {
      width:35%;
    }

    @include media-query($large-up) {
      width:20%;
    }
  }

  .basket-item-details {
    width:calc(65% - 20px);
    margin-left:10px;
    margin-right:10px;

    @include media-query($medium-up) {
      width:calc(55% - 1rem);
      margin-left: 1rem;
    }

    @include media-query($large-up) {
      display:flex;
      flex-direction:column;
      width:calc(70% - 1rem);
    }
  }

  .basket-actions {
    display:flex;
    flex-direction:column;
    width:10%;

    @include media-query($large-up) {
      display:none;
    }

    button {
      display:flex;
      flex-direction:column;
      justify-content:center;
      align-items:center;
      width:30px;
      height:30px;
      border-radius:50%;
      overflow:hidden;
      box-shadow:0 0 4px 2px rgba(#000, 0.3);
      margin-top:1rem;

      .label {
        display:none;
      }

    }

    .icon {
      width:10px;
      height:10px;

      path {
        fill:$brand-primary;
      }
    }

    .icon-close {
      width:12px;
      height:12px;
    }
  }

  .product-title {
    margin:0 0 .5rem;
    line-height:1.2;

    a {
      @include px-to-rem(10);
      text-transform:uppercase;
      text-decoration:none;
      font-weight:500;

      @include media-query($large-up) {
        @include px-to-rem(13);
      }
    }
  }

  .product-price-container {
    @include media-query($large-up) {
      order:-1;
    }
  }

  .product-price {
    margin-bottom:.5rem;
    span {
      display:inline-block;
    }

    .money:not(.original-price) {
      @include px-to-rem(14);
      font-weight:900;

      @include media-query($large-up) {
        @include px-to-rem(20);
      }
    }

    .original-price {
      position: relative;
      margin-left:.5rem;
      color:$gray-600;
      @include px-to-rem(12);

      &:before {
        display: block;
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: linear-gradient(to top left, transparent calc(50% - 1px), $gray-600, transparent calc(50% + 1px));
        content: '';
      }
    }

    &.sale .money:not(.original-price) {
      color:$brand-primary;
    }
  }

  .product-quantity {
    display:flex;
    flex-direction:row;
    align-items: center;
    margin-bottom: 1rem;

    .label {
      display:inline-block;
      margin-right:.5rem;
    }

    select {
      display:inline-block;
      padding-left:1rem;
      padding-right: calc(1.5rem + 10px);
      width:auto !important;
    }
  }

  .basket-actions-alt {
    display:none;

    @include media-query($large-up) {
      display:flex;
      flex-direction:row;
      align-items:center;
      margin-top:1rem;
    }

    button {
      margin-left:3rem;
      text-decoration:underline;
      @include px-to-rem(10);
      text-transform:uppercase;
    }
  }

  .cart-wishlist {
    display:flex;
    flex-direction:row;
    align-items:center;
    width:180px;
    height:52px;
    border:1px solid $gray-400;
    padding:.5rem .7rem;

    .smartwishlist {
      display:flex;
      flex-direction:row;
      flex-wrap:nowrap;
      align-items:center;
      height:inherit;
      width:inherit;
      color:$gray-600 !important;

      &:focus {
        outline:0;
      }

      &:after {
        display:inline-block;
        @include px-to-rem(12);
        margin-left:10px;
        text-transform:uppercase;
      }

      &.unbookmarked:after {
        content:'Save for later';
      }

      &.bookmarked:after {
        content:'Saved';
      }
    }
  }

  .ajax-feedback {
    @include media-query($large-up) {
      margin-bottom:0;
    }
  }

  .preorder-message {
    @include media-query($large-up) {
      margin-bottom:0;
    }
  }

}


/* ==========================================================================
#Basket Item Variants
========================================================================== */

body.template-cart .basket-item-variants {
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  align-items:center;
  margin-top:10px;

  select {
    height:auto;
    border:none;
    padding:0;
    margin:0;
    width:auto;
    text-transform:uppercase;
    @include px-to-rem(12);

    @include media-query($large-down) {
      background-image:none;
    }

    @include media-query($large-up) {
      background-position:right center;
      padding-right:20px !important;
    }
  }

  .product-colour-variant {
    @include px-to-rem(12);
    text-transform:uppercase;
    font-weight:500;
  }

  .product-quantity {
    margin-bottom:0;
    @include px-to-rem(12);

    .label {
      text-transform:uppercase;
      font-weight:500;
      line-height:13px;
    }
  }

  > * {
    margin-top:5px !important;
    margin-bottom:5px !important;
    height:16px !important;

    & + * {
      margin-left:5px !important;
      padding-left:5px !important;
      border-left:1px solid #000 !important;
    }
  }


}


/* ==========================================================================
#Basket Sidebar
========================================================================== */

#basket-sidebar {
  @include media-query($medium-down) {
    margin-top:2rem;
  }
}

.basket-sidebar-section {
  margin-top:2rem;

  @include media-query($large-down) {
    padding-left:15px;
    padding-right:15px;
  }

  h4 {
    font-family:$title-font;
    text-transform:uppercase;
    @include px-to-rem(25);
  }

  .content {
    padding:.5rem 0;
  }
}

/* ==========================================================================
#Basket Checkout
========================================================================== */
form.check-form {
  position: relative;
}
.btn-checkout {
  position: absolute;
  right: 0;
  top: 0;
  width: 300px;
}
.btn-checkout input {
  font-size: 16px !important;
  height: 40px !important;
}
.basket-total h2 {
  display: inline-block;
  width: 49%;
}

.basket-checkout .discount-text {
  font-size: 14px;
}
.basket-checkout {
  text-align:left;

  .basket-total {
    display:flex;
    flex-direction:row;
    align-items:left;
    justify-content: center;
    margin-top:1rem;
    margin-bottom:1rem;

    @include media-query($large-down) {
      padding:15px;
      background-color:$gray-300;
    }

    .label {
      font-family:$title-font;
      text-transform:uppercase;
      @include px-to-rem(25);
      margin-right:10px;
    }

    .money {
      @include px-to-rem(20);
      font-weight:500;
      margin-left:auto;
    }

  }
}

//BG: This is used in conjunction with functions in cart.js
#checkout-btn-placeholder {
  display:block;
  width:100%;
  height:55px;
  background-color:transparent;
}

#btn-checkout {
  width:calc(100% - 30px) !important;
  height:55px;
  margin-left:auto;
  margin-right:auto;
  background-color:#66FF96;
  color:#000;
  border:none;
  @include px-to-rem(25);

  @include media-query($large-up) {
    width:100% !important;
  }

  &:hover {
    background-color:#40d06d;
    color:#000;
  }


  @include media-query($large-down) {
    &.fixed {
      position:fixed;
      bottom:15px;
      left:15px;
      right:15px;
    }

    &.static {
      position:static;
    }
  }
}

#btn-checkout.static + #checkout-btn-placeholder {
  height:0;
}

/* ==========================================================================
#Ajax Feedback
========================================================================== */

.ajaxFeedbackCart {
  display:inline-block;
  padding:.5rem;
  margin-left: 1rem;

  &.success {
    background-color:$success;
    color:$successText;
    border-left:3px solid $successText;
  }

  &.warning {
    background-color:$warning;
    color:$warningText;
    border-left:3px solid $warningText;
  }
}

/* ==========================================================================
#Payment Icons
========================================================================== */

.payment-icons {
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  align-items: center;
  list-style:none;
  padding:0;
  margin:0;

  li {
    margin:.3rem;
  }

  img {
    max-width:48px;
    height:auto;
  }
}

.discount-text {
  @include px-to-rem(14);
}


/* ==========================================================================
#Mobile Edit Product Menu
========================================================================== */


#mobileEditOverlay {
  position:fixed;
  top:0;
  right:0;
  left:0;
  bottom:0;
  opacity:0;
  visibility:hidden;
  background-color:rgba(#000, .6);
  transition:all .3s ease-in-out;
  cursor:pointer;

  &.active {
    opacity:1;
    visibility:visible;
    z-index:100;
  }
