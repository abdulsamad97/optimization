.btn {
  display: inline-block;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  @include prefix(user-select, none, webkit moz ms spec);
  border: 2px solid transparent;
  padding: 0px 1.5rem;
  font-weight:500;
  text-transform: uppercase;
  text-decoration: none;
  line-height: 1.5;
