.featured-gallery {
  margin-top: 1rem;

  .featured-gallery-container {
    display:flex;
    flex-direction:row;
    flex-wrap:wrap;
    margin:0;
    max-width:100%;
  }

  .featured-gallery-item {
    width:100%;
    padding:0 7.5px 0;

    @include media-query($medium-up) {
      width:25%;
      padding:2.5px;
    }

    &:focus, a:focus, img:focus {
      outline:0;
    }

    img {
      //BG: not a fan, client request 
      @include media-query($widescreen-ultra) {
        max-width:none;
        width:100%;
      }
    }

  }

  .featured-gallery-item.video {
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;

    video {
      width:100%;
      height:auto;
    }
