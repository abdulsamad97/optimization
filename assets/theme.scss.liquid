/* ==========================================================================
#Product Images Container
========================================================================== */
@include media-query($large-up) {
  #product-images-container {
    display:flex;
    flex-direction:row;
    height:800px;
  }

  #product-thumbnails {
    width:100px;
    order:1;
  }

  #product-large-images {
    width:calc(100% - 105px);
    order:2;
  }
}

@include media-query($small-down) {
  #product-images-container {
    padding-left:0;
    padding-right:0;
  }
}

/* ==========================================================================
#Product Large Images
========================================================================== */

#product-large-images {
  margin:5px 0;
  cursor: zoom-in;

  @include media-query($large-up) {
    margin:0;
  }

  #modal-window-close {
    display:none;
  }

  .slider-count {
    display:none;
  }

  .slick-arrow {
    @extend .btn-plain;
    position: absolute;
    top:50%;
    transform: translateY(-50%);
    width:20px;
    height:20px;
    z-index:1;

    .icon, svg {
      height:inherit;
      width:inherit;
    }


    &.slick-prev {
      left:0;
    }

    &.slick-next {
      right:0;
    }
  }

  .img-wrap {
    position: relative;
  }

  .mobile-expand {
    position: absolute;
    right:10px;
    bottom:10px;
    left:auto;
    top:auto;
    z-index:100;

    &:focus {
      outline:0;
    }

    @include media-query($medium-up) {
      display:none;
    }
  }

  &:not(.slick-initialized) {
    .large-image {
      display:none;

      &:first-of-type {
        display:block;
      }
    }
  }

}


/* ==========================================================================
#Product Mobile Modal
========================================================================== */

//BG: mobile-modal class added via js from product.js 
#product-large-images.mobile-modal {
  position:fixed;
  top:60px;
  left:0;
  right:0;
  width:100%;
  z-index:140;

  @include media-query($large-up) {
    max-width:700px;
    left:50%;
    transform:translateX(-50%);
  }

  #modal-window-close {
    display:block;
    width:100%;
    text-align:center;
    text-transform:uppercase;
    color:#fff;
    @include px-to-rem(11);
  }

  .slider-count {
    width:100%;
    margin-top:2rem;
    text-align:center;


    p,span {
      color:#fff;
      text-transform:uppercase;
      font-family:$title-font;
      @include px-to-rem(25);
      margin:0;
    }
  }

  .mobile-expand {
    display:none;
  }

  .slider-count {
    display:block;
  }

  .slick-arrow {
    top:auto !important;
    bottom:3px;
    left:50% !important;
    right:auto !important;

    svg path{
      fill:#fff;
    }

    &.slick-next {
      transform:translateX(30px);
    }

    &.slick-prev {
      transform:translateX(-50px);
    }
  }

}


/* ==========================================================================
#Product Thumbnails
========================================================================== */

#product-thumbnails {
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  margin-left:-2.5px;
  margin-right:-2.5px;

  @include media-query($large-up) {
    flex-direction:column;
    justify-content:center;
    align-items:center;
    margin-left:0;
    margin-right:5px;
  }

  &.no-slick {
    flex-wrap:nowrap;

    .thumbnail-image {
      width:100%;
    }
  }

  .thumbnail-image {
    cursor: pointer;
    margin-left:2.5px;
    margin-right:2.5px;

    @include media-query($large-up) {
      margin-left: 0;
      margin-right: 0;
      margin-bottom: 5px;
    }

    &:focus {
      outline:0;
    }
  }

  @include media-query($large-down) {
    &.slick-initialized {
      &:before, &:after {
        position: absolute;
        top:0;
        bottom:0;
        width:10%;
        display:block;
        content:'';
        z-index:1;
      }

      &:before {
        left:0;
        background: rgb(255,255,255);
        background: linear-gradient(90deg, rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
      }

      &:after {
        right:0;
        background: rgb(255,255,255);
        background: linear-gradient(-90deg, rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
      }
    }
  }

}

/* ==========================================================================
#Product Images Slick
========================================================================== */

body.template-product {
  .slick-arrow {
    @extend .btn-plain;
    position: absolute;
    top:50%;
    transform: translateY(-50%);
    width:20px;
    height:20px;
    z-index:2;

    .icon, svg {
      height:inherit;
      width:inherit;
    }


    &.slick-prev {
      left:0;
    }

    &.slick-next {
      right:0;
    }
  }
}

/* ==========================================================================
#Product Title
========================================================================== */

body.template-product .product-detail .product-title, .product-quickview-frame .product-detail .product-title a {
  font-family: $body-font;
  @include px-to-rem(11);
  font-weight:500;
  text-transform:uppercase;
  text-align:left;
  margin-top:1.5rem;
  text-decoration:none;

  @include media-query($medium-up) {
    @include px-to-rem(13);
  }

  @include media-query($large-up) {
    @include px-to-rem(20);
    margin-top:0;
  }
}

/* ==========================================================================
#Product Price
========================================================================== */

body.template-product .product-detail .product-price, .product-quickview-frame .product-detail .product-price {
  margin-top:1.5rem;
  text-align:left;

  span {
    display:inline-block;
    text-align:left;
  }

  .money:not(.original-price) {
    display:block;
    @include px-to-rem(15);
    font-weight:900;

    @include media-query($large-up) {
      @include px-to-rem(26);
    }
  }

  .original-price {
    position: relative;
    margin-top:.5rem;
    color:$gray-600;
    @include px-to-rem(12);

    &:before {
      display: block;
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(to top left, transparent calc(50% - 1px), $gray-600, transparent calc(50% + 1px));
      content: '';
    }
  }

  &.sale .money:not(.original-price) {
    color:$brand-primary;
  }
