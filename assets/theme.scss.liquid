  font-family:$title-font;
  transition: all .3s ease-in-out;
  cursor: pointer;
  width:auto !important;

  @include media-query($large-up) {
    padding-left:2rem;
    padding-right:2rem;
    @include px-to-rem(24);
  }

  &:hover {
    text-decoration: none;
    color:inherit;
  }

  &:focus {
    outline:0;
  }
}

/* ==========================================================================
