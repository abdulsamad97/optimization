/* ==========================================================================
#Newsletter signup
========================================================================== */

#newsletter-signup {
  background-color: #e3d5cc;

  & + footer {
    margin-top:0;
  }

  .newsletter-content p {
    font-size:1.125rem;
  }

  h4 {
    display:inline-flex;
    flex-direction:row;
    flex-wrap:wrap;
    justify-content:center;
    align-items:center;
    font-family:$title-font;
    @include px-to-rem(60);

    img {
      display:inline-block;
      margin:0 1.3rem;

      @include media-query($small-down) {
        max-width:200px;
      }
    }

    @include media-query($large-up) {
      @include px-to-rem(70);
    }
  }

  form {
    display:flex;
    flex-direction:row;
    flex-wrap:nowrap;
    justify-content:center;
    align-items:center;
    border-bottom:0px solid #fff;
    max-width:500px;
    margin:0 auto 2rem;
    background: #fff;
    height:50px;

    .form-group {
      width:85%;
      margin:0;
    }

    input[type="email"] {
      border:0;
      background-color:transparent;
      text-transform:uppercase;
      color:#000;
      @include px-to-rem(13);
      font-weight:500;

      &::-webkit-input-placeholder { /* Chrome/Opera/Safari */
        color: #000;
      }

      &::-moz-placeholder { /* Firefox 19+ */
        color: #000;
      }

      &:-ms-input-placeholder { /* IE 10+ */
        color: #000;
      }

      &:-moz-placeholder { /* Firefox 18- */
        color: #000;
      }
    }

    button[type="submit"] {
      width: 15%;
      display: flex;
      border: none;
      padding: 0;
      margin: 0;
      text-decoration: none;
      background-color: transparent;
      color: #000;
      cursor: pointer;
      background: #000;
      -webkit-appearance: none;
      -moz-appearance: none;
      height: 50px;
      align-items: center;
      justify-content: center;

      &:focus {
        outline:0;
      }

      svg {

      }
    }
  }

  .gdpr-text p {
    @include px-to-rem(10);

    @include media-query($medium-up) {
      @include px-to-rem(12);
    }
  }

  .rte {
    @include px-to-rem(14);

    @include media-query($medium-up) {
      font-size:1rem; //16px
    }
  }


  .content__gdprLegal {
    @include px-to-rem(12);
  }

}

/* ==========================================================================
#Footer Columns
========================================================================== */

footer {
  margin-top: 1.5rem;
  //margin-bottom: 3rem;

  @include media-query($large-up) {
    margin-top: 1.5rem;
    background-color: #000;
  }
}

.footer-col {
  background-color: $gray-200;
  padding-top:.5rem;
  padding-bottom:.5rem;

  @include media-query($large-up) {
    background-color: transparent;
  }

  .footer-title {
    font-family: $title-font;
    text-transform:uppercase;
    @include px-to-rem(22);
    letter-spacing:.5px;

    @include media-query($large-up) {
      color:#fff;
      margin-top:2rem;
    }
  }

  .footer-content {
    margin-top: 1rem;
  }
}

.footer-col-social {
  background-color: #000;

  @include media-query($large-up) {
    background-color: transparent;
    padding-right:3rem;
  }

  @include media-query($widescreen) {
    padding-right:6rem;
  }

  @include media-query($large-down) {
    order:99;
  }

  .footer-content {
    margin-top:0;
  }

  .footer-social {
    margin-bottom:1rem;
  }

  .payment-icons {
    margin-bottom:1rem;

    img {
      max-width:48px;
      height:auto;
    }
  }

  .site-logo {
    padding:2rem 0 3rem;

    a {
      text-align:center;
    }

    svg {
      max-width:200px;

      path {
        fill:#fff;
      }
    }    
  }
}

body.user-gb .footer-content.ie-content, body.user-ie .footer-content.uk-content {
  display:none !important;
}



/* ==========================================================================
#Footer Plus Minus Icon
========================================================================== */

footer .plus-minus-icon {
  margin-left: .5rem;
  &:before,&:after {
    background-color: $title-font-color;
  }

  @include media-query($small-up) {
    display:none;
  }
}

.active > .plus-minus-icon {
  &:before, &:after {
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
  }
}

/* ==========================================================================
#Footer Links
========================================================================== */

.footer-links {
  list-style: none;
  margin: 0;
  padding: 0;

  li {
    margin-bottom:.7rem;
  }

  a {
    @include px-to-rem(11);
    text-decoration: none;
    text-transform:uppercase;
    color:#000;

    @include media-query($large-up) {
      color:#fff;
      @include px-to-rem(13);
    }

    &:hover {
      text-decoration: underline;
    }
  }
}

/* ==========================================================================
#Footer Copy
========================================================================== */

.footer-copy {
  padding:0 0 1rem;
  background-color:#000;
  border:1px solid #000;
  color:#fff;
  @include px-to-rem(10);

  @include media-query($large-up) {
    margin-top:2rem;
  }

  [class^="col-"] {
    color:inherit;
    text-align:center;
    text-transform:uppercase;
  }

  a {
    color:inherit;
    font-size:inherit;
    text-decoration: underline;
  }

}


/* ==========================================================================
#Footer Social Links
========================================================================== */

.footer-social {

  ul {
    display:flex;
    flex-direction:row;
    justify-content: center;
    align-items: center;
    padding:0;
    margin:0;

    @include media-query($large-up) {
      justify-content: space-between;
    }
  }

  li {
    display:block;
    transition:background-color .3s ease-in-out;
    border-radius:50%;

    &:hover {
      background-color: $brand-secondary;
    }

    & + li {
      margin-left: 15px;
    }
  }

  a {
    display:flex;
    width:100%;
    height:100%;
    justify-content: center;
    align-items: center;
    color:#fff;
  }

  .icon {
    width:25px;
    height:25px;

    path {
      fill:#fff;
    }

    @include media-query($large-up) {
      width:30px;
      height:30px;
    }
  }
}
