<div class="container-fluid py-3 {{ page.template_suffix }}">
	<div class="row">
		<div class="col-12 text-v-small">{% include 'breadcrumbs' %}</div>
	</div>
</div>

<header class="page-header">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-12 mb-md-0 col-md-8 col-lg-6 mx-auto text-center">
				<h1>{{ page.title }}</h1>
			</div>
		</div>
	</div>
</header>

<div class="container">
	<div class="row mb-5">
		<div class="col-12 mx-auto">
			<div class="row no-gutters">
                <div class="col-12 mt-4">
                    <div class="text-center mb-4">
                        {% if shop.metafields.okendo.WidgetPreRenderBodyStyleTags != blank %}{{ shop.metafields.okendo.WidgetPreRenderBodyStyleTags }}{% endif %}<div data-oke-widget></div>
                    </div>
                </div>
			</div>
		</div>
	</div>
</div>